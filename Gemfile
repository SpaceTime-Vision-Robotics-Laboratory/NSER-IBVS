source 'https://rubygems.org'

# GitHub Pages gem - includes Jekyll and compatible plugins
gem 'github-pages', group: :jekyll_plugins

# Additional Jekyll plugins
gem 'jekyll-feed'
gem 'jekyll-sitemap'

# Required for Ruby 3.0+
gem 'webrick'

# Development gems
group :development do
  gem 'jekyll-admin'
end